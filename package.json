{
  "name": "eslint-plugin-feature-flags",
  "version": "0.0.0",
  "private": true,
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "build": "tsc --build tsconfig.build.json && turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test": "turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "clean": "turbo run clean && rimraf node_modules",
    "clean:build": "turbo run clean && rimraf .turbo tsconfig.build.tsbuildinfo"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "packageManager": "pnpm@10.11.1",
  "devDependencies": {
    "turbo": "^2.5.4",
    "prettier": "^3.0.0",
    "rimraf": "^5.0.5",
    "typescript": "^5.4.2"
  }
}
